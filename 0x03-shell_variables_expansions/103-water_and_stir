#!/bin/bash

WATER="$WATER"
STIR="$STIR"

# Remove any non-numeric characters from WATER and STIR
WATER=$(echo "$WATER" | tr -cd '[:digit:]')
STIR=$(echo "$STIR" | tr -cd '[:digit:]')

# Convert WATER and STIR to base 10 and perform the addition
result=$((10#${WATER} + 10#${STIR}))

# Convert the result to base bestchol
result_base_bestchol=$(echo "obase=bestchol; $result" | bc)

# Print the result
echo "$result_base_bestchol"
